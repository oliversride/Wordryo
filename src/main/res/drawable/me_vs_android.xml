<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <bitmap android:src="@drawable/mevsandroid" android:tint="?android:attr/colorPressedHighlight" />
    </item>
    <item android:state_focused="true">
        <bitmap android:src="@drawable/mevsandroid" android:tint="?android:attr/colorFocusedHighlight" />
    </item>
    <item>
        <bitmap android:src="@drawable/mevsandroid" android:tint="@color/white" />
    </item>
</selector>